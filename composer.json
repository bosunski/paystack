{
    "name": "xeviant/paystack",
    "description": "A Paystack API Wrapper",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Olatunbosun Egberinde",
            "email": "bosunski@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": "^8.0",
        "psr/cache": "^2.0.0",
        "php-http/discovery": "^1.14.1",
        "php-http/cache-plugin": "^1.7.5",
        "php-http/guzzle6-adapter": "^2.0.0",
        "php-http/httplug":  "^2.3.0",
        "illuminate/container": "^8.0|^9.0",
        "illuminate/support": "^8.0|^9.0",
        "league/event": "^2.0.0"
    },
    "autoload": {
        "psr-4": {
            "Xeviant\\Paystack\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Xeviant\\Paystack\\Tests\\": "test/"
        }
    },
    "prefer-stable": true,
    "require-dev": {
        "phpunit/phpunit": "^9.5.0",
        "php-http/mock-client": "^1.5.0",
        "cache/array-adapter": "^1.2.0"
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    }
}
